{
  "name": "akita-react-hooks-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@datorama/akita": "^4.19.2",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.2",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.0",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "typescript": "3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} && eslint src/**/*.{js,jsx}",
    "prettier": "prettier \"**/*.{ts,tsx,scss,css,js,json,md}\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": false,
    "basePath": "./src/core/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@datorama/akita-cli": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react-hooks": "^2.4.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.16.4",
    "tslint": "^6.0.0",
    "tslint-config-react-app": "^0.0.0",
    "tslint-react": "^4.2.0"
  }
}
